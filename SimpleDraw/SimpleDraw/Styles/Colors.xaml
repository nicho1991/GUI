<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- The brush for background grey -->
    <Color x:Key="BackGroundGrey">#efefef</Color>
    <SolidColorBrush x:Key="BackGroundGreyBrush" Color="{StaticResource BackGroundGrey}"></SolidColorBrush>
</ResourceDictionary>